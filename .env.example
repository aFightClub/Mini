# GitHub Credentials
GITHUB_TOKEN=your_github_personal_access_token_here

# Apple Developer Credentials for macOS signing and notarization
APPLE_ID=your_apple_id@example.com
APPLE_ID_PASSWORD=your_app_specific_password_or_keychain_reference
APPLE_TEAM_ID=your_team_id
APPLE_DEVELOPER_IDENTITY="Developer ID Application: Your Name (XXXXXXXXXX)"

# Electron Forge signing configuration
CSC_LINK=path_to_your_certificate.p12
CSC_KEY_PASSWORD=your_certificate_password
CSC_IDENTITY_AUTO_DISCOVERY=true

# Skip signing during development (set to false for production builds)
CSC_FORCE_SIGN=false 